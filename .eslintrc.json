{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:import/errors"
  ],
  "overrides": [
    {
      "files": ["test.js"],
      "env": { "jest": true }
    }
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script"
  },
  "rules": {
    "import/order": ["error", { "newlines-between": "always" }]
  }
}
